pico-8 cartridge // http://www.pico-8.com
version 42
__lua__
-- experiments with tables

table_a = {
	x=1,y=2,z=3,
	cat="neko"}

-- access all key value pairs
for k,v in pairs(table_a) do
	print(k..": "..v)
end

-- access by key
print("by key: "..table_a["cat"])
-- access non-existing key
if table_a["dog"] == nil then
	print("missing dog")
	print(table_a["dog"])
end

