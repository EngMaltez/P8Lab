version 42
__lua__
--dungeon roguelike
--zetlam
-- 3 pomodoro

function _init()
p_x = 3
p_y = 2
end

function _update()
	if btnp(⬅️) then
		p_x -= 1
	end
	if btnp(➡️) then
		p_x += 1
	end
	if btnp(⬆️) then
		p_y -= 1
	end
	if btnp(⬇️) then
		p_y += 1
	end
end

function _draw()
	cls()
	palt(0,false)
	map()
	spr(49,8*p_x,8*p_y)
end

